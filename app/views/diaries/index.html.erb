<h1>labo_diary</h1>
<h3>投稿一覧</h3>
<%= link_to "新規投稿へ", diaries_new_path %>
<div class="diaries-container">
  <% @diaries.each do |t| %>
    <div class="diary">
      <%= t.body %>
      <%= t.created_at %>
      <%= link_to "詳細へ", diary_path(t.id) %>
      <%= link_to "編集する", edit_diary_path(t.id) %>
      <%= button_to "削除する", diary_path(t.id), method: :delete %>
    </div>
  <% end %>
</div>